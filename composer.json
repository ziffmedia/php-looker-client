{
    "name": "ziffmedia/looker",
    "type": "library",
    "require": {
        "symfony/http-client": "^6.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ZiffMedia\\Looker\\": "src/"
        }
    },
